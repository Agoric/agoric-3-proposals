{
  "type": "module",
  "devDependencies": {},
  "dependencies": {
    "@endo/zip": "^0.2.35",
    "ava": "^5.3.1",
    "better-sqlite3": "^8.5.1",
    "execa": "^7.2.0",
    "tmp": "^0.2.1"
  },
  "scripts": {
    "agops": "yarn --cwd /usr/src/agoric-sdk/ --silent agops",
    "upgrade-tests-1": "ava --serial agoric-upgrade*/**/pre.test.js agoric-upgrade*/**/actions.test.js agoric-upgrade*/**/post.test.js",
    "upgrade-tests-2": "if [ -d agoric-upgrade-11 ]; then ava --serial agoric-upgrade-11/mn2.test.js agoric-upgrade-11/add-collateral.test.js; fi",
    "upgrade-tests": "yarn run upgrade-tests-1 && yarn run upgrade-tests-2"
  },
  "ava": {
    "files": [
      "*/**/pre.test.js",
      "*/**/actions.test.js",
      "*/**/post.test.js"
    ],
    "timeout": "30m"
  },
  "typeCoverage": {
    "atLeast": 0
  }
}
